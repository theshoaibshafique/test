{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A minimal react-redux boilerplate with all the best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/flexdinesh/react-redux-boilerplate.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
